<Page
    x:Class="Window.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Window"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="261,215,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Height="59" Width="99"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="261,311,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Height="60" Width="99"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,98,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="57" Width="129"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="10,215,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="10,351,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="261,98,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" TextChanged="textBox_TextChanged" Height="57" Width="99"/>

    </Grid>
</Page>
